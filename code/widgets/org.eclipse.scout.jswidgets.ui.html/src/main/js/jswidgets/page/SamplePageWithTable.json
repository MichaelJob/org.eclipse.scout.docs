          "fields": [
            {
              "id": "LoadingDelayCheckBox",
              "objectType": "CheckBoxField",
              "labelVisible": false,
              "statusVisible": false,
              "cssClass": "no-mandatory-indicator",
              "gridDataHints": {
                "useUiWidth": true,
                "weightX": 0
              }
            },
            {
              "id": "LoadingDelayField",
              "objectType": "NumberField",
              "label": "Loading delay [ms]",
              "labelPosition": "${const:ON_FIELD}",
              "labelVisible": false,
              "statusVisible": false,
              "enabled": false,
              "gridDataHints": {
                "horizontalAlignment": -1
              }
            }
          ]
        }
      }
    ]
  }
